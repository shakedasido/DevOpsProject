{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 3],
        "block-scoped-var": "off",
        "vars-on-top": "off",
        "no-continue": "off",
        "no-await-in-loop": "off",
        "no-redeclare": "off",
        "max-len": "off",
        "no-param-reassign": "off",
        "no-unused-vars": "off",
        "no-undef": "off",
        "camelcase": "off"
    }
    
    
}
